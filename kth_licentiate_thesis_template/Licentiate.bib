isbn = {0018-9200},
issn = {00189200},
journal = {IEEE J. Solid-State Circuits},
keywords = {Adaptive dead-time,Boost converter,Discontinuous-conduction mode,Sub-1V},
number = {9},
pages = {2036--2046},
title = {{A 0.9-V input discontinuous-conduction-mode boost converter with CMOS-control rectifier}},
volume = {43},
year = {2008}
}
