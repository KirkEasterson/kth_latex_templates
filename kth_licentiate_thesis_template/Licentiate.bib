issn = {0140-0118},
journal = {Med. Biol. Eng. Comput.},
keywords = {Artificial,Electric Power Supplies,Electronics,Equipment Design,Feasibility Studies,Humans,Medical,Pacemaker},
month = may,
number = {3},
