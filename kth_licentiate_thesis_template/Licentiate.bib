issn = {08858993},
journal = {IEEE Trans. on Power Electron.},
keywords = {Adaptive gate switching control (AGSC),dc-dc converter,dead-time controller (DTC),discontinuous conduction mode (DCM),zero-current detector (ZCD)},
number = {3},
pages = {1311--1320},
title = {{Adaptive gate switching control for discontinuous conduction mode DC-DC converter}},
volume = {29},
year = {2014}
